#PROG_TYPE verb_progp move hands
#PROG_BODY 
if isghost($n)
 mpechoat $n Your ghostly hands pass right through the clock.
 mpverbstop
 break
endif
if level($n) < 19
or isimmort($n)
else
 mpechoat $n The hands won't budge!
 mpverbstop
 break
endif
if roomvalue(2) == 0
 mpvalueset 2 1
endif
if isopen() == 5
 mpechoat $n The hands won't budge!
 mpverbstop
else
 if compstr(3) == A
 or compstr(3) == B
 or compstr(3) == C
 or compstr(3) == D
 else
  mpechoat $n Move the hands to which position? (move hands A, B, C or D)
  mpverbstop
  break
 endif
 mpvalueset 1 0
 if roomvalue(0) == 1
  if compstr(3) == B
   mpvalueset 1 1
  endif
 endif
 if roomvalue(0) == 2
  if compstr(3) == C
   mpvalueset 1 1
  endif
 endif
 if roomvalue(0) == 3
  if compstr(3) == A
   mpvalueset 1 1
  endif
 endif
 if roomvalue(0) == 4
  if compstr(3) == D
   mpvalueset 1 1
  endif
 endif
 if roomvalue(1) == 1
  mpechoat $n You move the hands to their new position, and they click into place!
  mpechoaround $n $N move the hands of the lowest clock.
  mpecho The heavy iron trapdoor slowly grinds open, revealing a downward-spiralling staircase.
  mpflag exit down locked
  mpflag exit down closed
  mpat 26652 mpflag exit up closed
  mpvalueset 2 1
  mpverbstop
