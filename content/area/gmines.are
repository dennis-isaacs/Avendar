mpdamage $n 5 50 150
mplag $n 3
mptransfer $n 22677
mpechoaround $n $N falls from above, hitting the ground hard.
mpforce $n look
else
mpechoat $n You quickly clamber down the handholds, narrowly escaping the fight!
mpechoaround $n $N quickly clambers down the shaft, narrowly escaping the fight!
mptransfer $n 22677
mpechoaround $n $N climbs down the shaft and drops onto the floor.
mpforce $n look
endif
else
mpechoat $n You grasp the rusty handholds and carefully let yourself down the shaft.
mpechoaround $n $N grasps the rusty handholds and carefully lets $mself down the shaft.
mptransfer $n 22677
mpechoaround $n $N climbs down the shaft and drops onto the floor.
mpforce $n look
endif
~
>verb_prog p down~
if isnpc($n)
or isfighting($n)
else
if isimmort($n)
or isghost($n)
mpinterpret $n $x
else
if isflying($n)
mpechoat $n You fly down the vertical shaft.
mpechoaround $n $N flies down the vertical shaft.
mptransfer $n 22677
mpechoaround $n $N arrives from above, flying down the vertical shaft.
mpforce $n look
else
mpechoat $n You jump down the vertical shaft, landing hard on the ground below!
mpechoaround $n $N jumps down the vertical shaft!
mplag $n 2
mpdamage $n 1 1 24
mptransfer $n 22677
mpechoaround $n $N falls from above, hitting the ground hard.
mpforce $n look
endif
endif
endif
~
