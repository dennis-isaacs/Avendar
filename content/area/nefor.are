if isowner($n)
  mpechoat $n You roll $I.
  mpechoaround $n $N rolls $I.
  mplag $n 1
  if isbitset($n) == 24005
  else
    if rand(20)
      mpechoat $n For just a moment, the pattern of the bones seems nearly recognizable.
      mpbitset $n 24005 1
    else
      mpechoat $n You feel utterly incompetent to read the bones.
    endif
    break
  endif
  if isbitset($n) == 24006
  else
    if rand(20)
      mpechoat $n The pattern of the bones begins to seem somewhat more recognizable.
      mpbitset $n 24006 1
    else
      mpechoat $n You feel somewhat incompetent to read the bones.
    endif
    break
  endif
  if isbitset($n) == 24007
  else
    if rand(20)
      mpechoat $n You peer closely at the bones, and the patterns falls into place!
      mpbitset $n 24007 1
    else
      mpechoat $n You can very nearly read the bones.
      break
    endif
  endif
  mpvaluerand 0 1 13
  if value(0) == 1
    mpvalueset bones the helmet
  endif
  if value(0) == 2
    mpvalueset bones the spear
  endif
  if value(0) == 3
