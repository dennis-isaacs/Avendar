  endif
  if value(0) == 7
    mpvalueset bones the cloud
  endif
  if value(0) == 8
    mpvalueset bones the candle
  endif
  if value(0) == 9
    mpvalueset bones the diamond
  endif
  if value(0) == 10
    mpvalueset bones the griffin
