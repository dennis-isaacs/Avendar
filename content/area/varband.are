  if isowner($n)
    mpechoaround $n $N quaffs a tiny mauve vial.
    mpechoat $n You quaff a tiny mauve vial.
    if isbitset($n) == 6125
      if isbitset($n) == 6126
        mpvalueset 9 15
        mpvalueset 8 50
        mpvalueset 7 100
      else
        mpechoat $n The unctuous, slippery drink is sweetly familiar, sliding down your throat. 
        mpbitset $n 6126 1
        mpvalueset 9 1
        mpvalueset 8 20
        mpvalueset 7 40
      endif
    else
      mpechoat $n The oil of Esithi tastes strange... but it isn\@t half bad. 
      mpechoat $n In fact... it\@s quite good!
      mpbitset $n 6125 1
      mpvalueset 9 -1
      mpvalueset 8 5
      mpvalueset 7 35
    endif
    if iscarrying($n) == 6102
    or iscarrying($n) == 6328
    or iscarrying($n) == 6329
      mpechoat $n You feel much, much better.
      mpechoaround $n $N sighs with contentment.
      if iscarrying($n) == 6102
        mpoload 6102 on $n
        mpvalueset 3 3
        mpforce $n esithi_purge
        mppurge self
        opverbstop
        break
      else
        mpvaluerand 2 0 100
        if value(2) < $9
          mpoload 6102 on $n
        else
          if value(2) < $8
            mpoload 6328 on $n
          else
            if value(2) < $7
              mpoload 6329 on $n
            endif
          endif
        endif
      endif
    endif
    if iscarrying($n) == 6330
    else
      mpoload 6330 on $n
    endif
    mppurge self
    opverbstop
  else
    mpnextinterp
  endif
else
  mpnextinterp
