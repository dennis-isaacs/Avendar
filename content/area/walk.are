      mpvalueset 1 2
    endif
    \\*** Apply the line of data, which starts with \#maze\#, activating the maze verb_prog for the row
    $d
    mpnextdata
  endloop
  \\* STRING ARTIFACT
  mpgoto 17236
  mpdesc yahoyecrystal %themeNam image
  mpdesc yahoyecrystal %themeNam images
  mpdesc yahoyecrystal %themeNam facet
  mpecho The image in the carved crystal flickers and changes.
  mpgoto 26140
  \\* tell Dovolente Entry: %entrypoint   Maze: %mazeNum   Theme: %themeNam   Layout: %themeType
endif
