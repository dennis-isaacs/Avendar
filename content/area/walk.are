  endif
  if value(7) == 2
    mpvalueset entrypoint 26155
    mpvalueset exitpoint 26171
  endif
  if value(7) == 3
    mpvalueset entrypoint 26171
    mpvalueset exitpoint 26155
  endif
  if value(7) == 4
    mpvalueset entrypoint 26175
    mpvalueset exitpoint 26151
  endif
  mpat 17236 mpforce yaoye_entrybot mpvalueset 0 %entrypoint
  mpgoto %exitpoint
  mpoload 26143
  mpstring obj walkportal long A %portaldesc is here.
  mpstring obj walkportal short a %portaldesc
  mpstring obj walkportal name %portaldesc
  \\* LOAD ENDPOINT NASTY MOB
  mpmload 26143
  mpstring mob endmob long A %mob3 guards a %portaldesc.
  mpstring mob endmob short a %mob3
  mpstring mob endmob name %mob3 yaoye_mob
  \\* SET UP NEW EXITS
  mpvalueset 0 26151
  mpvaluerand mazeNum 1 30
  mpsetdata %mazeNum
  \\*** Determine even or odd row. Eeven requires e/w exits, odd requires n/s or u/d exits
  loop 1 to 9
    mpvalueset 6 $v1
    if value(6) == 1
    or value(6) == 3
    or value(6) == 5
    or value(6) == 7
    or value(6) == 9
      mpvalueset 1 1
    else
