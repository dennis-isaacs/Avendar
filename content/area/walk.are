  mpsetdata %themeType
  loop 1 to %themeNum
    mpvalueset themeNam $d
    mpnextdata
    mpvalueset sector $d
    mpnextdata
    mpvalueset roomflag1 $d
    mpnextdata
    mpvalueset roomflag2 $d
    mpnextdata
    mpvalueset trash1 $d
    mpnextdata
    mpvalueset trash2 $d
    mpnextdata
